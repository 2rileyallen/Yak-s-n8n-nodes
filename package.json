{
  "name": "yaks-n8n-nodes",
  "version": "0.1.0",
  "description": "",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "",
    "email": "2rileyallen@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/2rileyallen/Yak-s-n8n-nodes.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "index.js",
  "scripts": {
    "clean": "npx rimraf dist",
    "build": "npx rimraf dist && tsc && npm run postbuild:assets",
    "prepare": "npm run build",
    "postbuild:assets": "npx cpy \"nodes/**/*.!(ts)\" dist/nodes --parents",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ComfyApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Yak-ChatterboxTTS/ChatterboxTTS.node.js",
      "dist/nodes/Yak-ComfyUI-Server/ComfyUI_Server.node.js",
      "dist/nodes/Yak-ComfyUI/ComfyUI.node.js",
      "dist/nodes/Yak-FFMPEG/FFMPEG.node.js",
      "dist/nodes/Yak-MuseTalk/MuseTalk.node.js",
      "dist/nodes/Yak-VocalRemover/VocalRemover.node.js",
      "dist/nodes/Yak-WhisperSTT/WhisperSTT.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/parser": "~8.32.0",
    "cpy-cli": "^5.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "rimraf": "^6.0.1"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "ws": "^8.18.3",
    "axios": "^1.7.2",
    "form-data": "^4.0.0"
  }
}
